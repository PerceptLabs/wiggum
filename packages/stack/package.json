    "react-hook-form": "^7.57.0",
    "@hookform/resolvers": "^5.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "typescript": "~5.9.3",
    "tailwindcss": "^4.1.18"
  }
