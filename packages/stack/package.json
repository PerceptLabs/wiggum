{
  "name": "@wiggum/stack",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "UI component library and utilities for Wiggum",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./components/ui": {
      "types": "./src/components/ui/index.ts",
      "import": "./src/components/ui/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts"
    },
    "./lib": {
      "types": "./src/lib/index.ts",
      "import": "./src/lib/index.ts"
    },
    "./tailwind": {
      "types": "./tailwind.config.ts",
      "import": "./tailwind.config.ts"
    },
    "./tokens": {
      "import": "./tokens.json"
    }
  },
  "files": [
    "src",
    "tokens.json",
    "tailwind.config.ts"
  ],
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
